@{
    Numeric = @{
        Printable = @"
+---+---+---+
| 7 | 8 | 9 |
+---+---+---+
| 4 | 5 | 6 |
+---+---+---+
| 1 | 2 | 3 |
+---+---+---+
  ! | 0 | A |
    +---+---+
"@
        Condensed = @"
789
456
123
!0A
"@
        Moves = @{
            '7' = @{X = 0; Y = 0}
            '8' = @{X = 1; Y = 0}
            '9' = @{X = 2; Y = 0}
            '4' = @{X = 0; Y = 1}
            '5' = @{X = 1; Y = 1}
            '6' = @{X = 2; Y = 1}
            '1' = @{X = 0; Y = 2}
            '2' = @{X = 1; Y = 2}
            '3' = @{X = 2; Y = 2}
            '0' = @{X = 1; Y = 3}
            'A' = @{X = 2; Y = 3}
        }
        Gap = @{X = 0; Y = 3}
    }
    Directional = @{
        Printable = @"
    +---+---+
  ! | ^ | A |
+---+---+---+
| < | v | > |
+---+---+---+
"@
        Condensed = @"
!^A
<v>
"@
        Moves = @{
            '^' = @{X = 1; Y = 0}
            'A' = @{X = 2; Y = 0}
            '<' = @{X = 0; Y = 1}
            'v' = @{X = 1; Y = 1}
            '>' = @{X = 2; Y = 1}
        }
        Gap = @{X = 0; Y = 0}
    }
}
